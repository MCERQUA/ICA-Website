<script>
function calculateCost() {
  // Get form values
  const projectType = document.getElementById('projectType').value;
  const foamType = document.getElementById('foamType').value;
  const squareFootage = parseFloat(document.getElementById('squareFootage').value);
  const thickness = parseFloat(document.getElementById('thickness').value);
  const accessibility = document.getElementById('accessibility').value;
  const removal = document.getElementById('removal').value;
  
  // Validate required fields
  if (!projectType || !foamType || !squareFootage || !thickness) {
    alert('Please fill in all required fields to calculate your estimate.');
    return;
  }
  
  // Base pricing per square foot by project type and foam type
  const basePricing = {
    'attic': {
      'open-cell': { low: 2.50, high: 4.00 },
      'closed-cell': { low: 3.50, high: 5.50 },
      'hybrid': { low: 4.00, high: 6.00 }
    },
    'walls': {
      'open-cell': { low: 3.00, high: 4.50 },
      'closed-cell': { low: 4.00, high: 6.00 },
      'hybrid': { low: 4.50, high: 6.50 }
    },
    'crawlspace': {
      'open-cell': { low: 3.50, high: 5.00 },
      'closed-cell': { low: 4.50, high: 7.00 },
      'hybrid': { low: 5.00, high: 7.50 }
    },
    'basement': {
      'open-cell': { low: 3.00, high: 4.50 },
      'closed-cell': { low: 4.00, high: 6.50 },
      'hybrid': { low: 4.50, high: 7.00 }
    },
    'commercial': {
      'open-cell': { low: 3.50, high: 5.50 },
      'closed-cell': { low: 4.50, high: 7.50 },
      'hybrid': { low: 5.00, high: 8.00 }
    }
  };
  
  // Get base pricing
  const basePrice = basePricing[projectType][foamType];
  let lowPrice = basePrice.low;
  let highPrice = basePrice.high;
  
  // Thickness multiplier (thicker = higher cost per sq ft)
  const thicknessMultiplier = 1 + (thickness - 2) * 0.1;
  lowPrice *= thicknessMultiplier;
  highPrice *= thicknessMultiplier;
  
  // Access difficulty multiplier
  const accessMultipliers = {
    'easy': 1.0,
    'moderate': 1.15,
    'difficult': 1.25,
    'extreme': 1.40
  };
  const accessMultiplier = accessMultipliers[accessibility] || 1.0;
  lowPrice *= accessMultiplier;
  highPrice *= accessMultiplier;
  
  // Removal cost addition
  const removalCosts = {
    'none': 0,
    'minimal': 0.75,
    'standard': 2.00,
    'extensive': 3.25
  };
  const removalCost = removalCosts[removal] || 0;
  
  // Calculate total costs
  const materialLowCost = lowPrice * squareFootage;
  const materialHighCost = highPrice * squareFootage;
  const totalRemovalCost = removalCost * squareFootage;
  
  const totalLowCost = materialLowCost + totalRemovalCost;
  const totalHighCost = materialHighCost + totalRemovalCost;
  const totalAvgCost = (totalLowCost + totalHighCost) / 2;
  
  // Calculate energy savings (estimated)
  const rValuePerInch = foamType === 'closed-cell' ? 6.5 : 3.7;
  const totalRValue = rValuePerInch * thickness;
  const energySavings = Math.round(squareFootage * 0.15 * (totalRValue / 30) * 12); // Simplified calculation
  
  // Display results
  document.getElementById('lowEstimate').textContent = '$' + Math.round(totalLowCost).toLocaleString();
  document.getElementById('avgEstimate').textContent = '$' + Math.round(totalAvgCost).toLocaleString();
  document.getElementById('highEstimate').textContent = '$' + Math.round(totalHighCost).toLocaleString();
  document.getElementById('energySavings').textContent = '$' + energySavings.toLocaleString();
  
  // Project details
  const details = `
    <p><strong>Project:</strong> ${projectType.charAt(0).toUpperCase() + projectType.slice(1)} insulation with ${foamType} spray foam</p>
    <p><strong>Area:</strong> ${squareFootage.toLocaleString()} square feet</p>
    <p><strong>Thickness:</strong> ${thickness} inches (approximately R-${Math.round(totalRValue)} total)</p>
    <p><strong>Access:</strong> ${accessibility.charAt(0).toUpperCase() + accessibility.slice(1)} difficulty</p>
    ${removal !== 'none' ? `<p><strong>Removal:</strong> ${removal} old insulation removal included</p>` : ''}
  `;
  document.getElementById('projectDetails').innerHTML = details;
  
  // Show results
  document.getElementById('results').style.display = 'block';
  document.getElementById('results').scrollIntoView({ behavior: 'smooth', block: 'start' });
}
</script>

