<!-- Calculator JavaScript -->
<script>
document.addEventListener('DOMContentLoaded', function() {
  // Calculator elements
  const sqFootageInput = document.getElementById('square-footage');
  const foamTypeInputs = document.querySelectorAll('input[name="foam-type"]');
  const thicknessSelect = document.getElementById('thickness');
  const energyBillInput = document.getElementById('energy-bill');
  
  // Result elements
  const materialCostEl = document.getElementById('material-cost');
  const laborCostEl = document.getElementById('labor-cost');
  const prepCostEl = document.getElementById('prep-cost');
  const totalCostEl = document.getElementById('total-cost');
  const monthlySavingsEl = document.getElementById('monthly-savings');
  const annualSavingsEl = document.getElementById('annual-savings');
  const tenYearSavingsEl = document.getElementById('ten-year-savings');
  const paybackPeriodEl = document.getElementById('payback-period');

  function calculateCosts() {
    const sqFt = parseFloat(sqFootageInput.value) || 2000;
    const thickness = parseFloat(thicknessSelect.value) || 3;
    const energyBill = parseFloat(energyBillInput.value) || 450;
    const foamType = document.querySelector('input[name="foam-type"]:checked').value;
    
    // Board feet calculation
    const boardFeet = sqFt * thickness;
    
    // Cost ranges based on foam type
    let materialCostPerBF, laborCostPerBF;
    if (foamType === 'open') {
      materialCostPerBF = { min: 0.35, max: 0.65 };
      laborCostPerBF = { min: 0.25, max: 0.40 };
    } else {
      materialCostPerBF = { min: 1.00, max: 2.00 };
      laborCostPerBF = { min: 0.30, max: 0.50 };
    }
    
    // Calculate costs
    const materialCost = {
      min: boardFeet * materialCostPerBF.min,
      max: boardFeet * materialCostPerBF.max
    };
    
    const laborCost = {
      min: boardFeet * laborCostPerBF.min,
      max: boardFeet * laborCostPerBF.max
    };
    
    const prepCost = {
      min: sqFt * 0.20,
      max: sqFt * 0.30
    };
    
    const totalCost = {
      min: materialCost.min + laborCost.min + prepCost.min,
      max: materialCost.max + laborCost.max + prepCost.max
    };
    
    // Calculate savings (40-50% of current bill)
    const savingsPercent = { min: 0.40, max: 0.50 };
    const monthlySavings = {
      min: energyBill * savingsPercent.min,
      max: energyBill * savingsPercent.max
    };
    
    const annualSavings = {
      min: monthlySavings.min * 12,
      max: monthlySavings.max * 12
    };
    
    const tenYearSavings = {
      min: annualSavings.min * 10,
      max: annualSavings.max * 10
    };
    
    const paybackYears = {
      min: totalCost.min / annualSavings.max,
      max: totalCost.max / annualSavings.min
    };
    
    // Update display
    materialCostEl.textContent = `$${formatNumber(materialCost.min)} - $${formatNumber(materialCost.max)}`;
    laborCostEl.textContent = `$${formatNumber(laborCost.min)} - $${formatNumber(laborCost.max)}`;
    prepCostEl.textContent = `$${formatNumber(prepCost.min)} - $${formatNumber(prepCost.max)}`;
    totalCostEl.textContent = `$${formatNumber(totalCost.min)} - $${formatNumber(totalCost.max)}`;
    monthlySavingsEl.textContent = `$${formatNumber(monthlySavings.min)} - $${formatNumber(monthlySavings.max)}`;
    annualSavingsEl.textContent = `$${formatNumber(annualSavings.min)} - $${formatNumber(annualSavings.max)}`;
    tenYearSavingsEl.textContent = `$${formatNumber(tenYearSavings.min)} - $${formatNumber(tenYearSavings.max)}`;
    paybackPeriodEl.textContent = `${paybackYears.min.toFixed(1)} - ${paybackYears.max.toFixed(1)} years`;
    
    // Update progress bar
    const progressBar = document.querySelector('.bg-primary-600');
    if (progressBar) {
      const avgPayback = (paybackYears.min + paybackYears.max) / 2;
      const progressPercent = Math.min((1 / avgPayback) * 100, 100);
      progressBar.style.width = `${progressPercent}%`;
    }
  }
  
  function formatNumber(num) {
    return Math.round(num).toLocaleString();
  }
  
  // Event listeners
  sqFootageInput.addEventListener('input', calculateCosts);
  thicknessSelect.addEventListener('change', calculateCosts);
  energyBillInput.addEventListener('input', calculateCosts);
  foamTypeInputs.forEach(input => {
    input.addEventListener('change', calculateCosts);
  });
  
  // Radio button visual update
  foamTypeInputs.forEach(input => {
    input.addEventListener('change', function() {
      document.querySelectorAll('.w-2\\.5.h-2\\.5').forEach(dot => {
        dot.classList.add('hidden');
      });
      if (this.checked) {
        this.parentElement.querySelector('.w-2\\.5.h-2\\.5').classList.remove('hidden');
      }
    });
  });
  
  // Initial calculation
  calculateCosts();
});
</script>

