<!-- ROI Calculator Script -->
<script>
document.addEventListener('DOMContentLoaded', function() {
    const sqftInput = document.getElementById('sqft');
    const billInput = document.getElementById('bill');
    const typeSelect = document.getElementById('type');
    
    function calculateROI() {
        const sqft = parseFloat(sqftInput.value) || 2000;
        const bill = parseFloat(billInput.value) || 350;
        const type = typeSelect.value;
        
        let savingsPercent = 0.35; // Default 35%
        if (type === 'spray-foam') savingsPercent = 0.40;
        if (type === 'blown-in') savingsPercent = 0.25;
        if (type === 'combo') savingsPercent = 0.40;
        
        const monthlySavings = bill * savingsPercent;
        const annualSavings = monthlySavings * 12;
        const fiveYearSavings = annualSavings * 5;
        
        // Estimate cost based on sq ft and type
        let costPerSqFt = 1.50; // Default
        if (type === 'spray-foam') costPerSqFt = 2.50;
        if (type === 'blown-in') costPerSqFt = 1.00;
        if (type === 'combo') costPerSqFt = 2.00;
        
        const estimatedCost = sqft * costPerSqFt;
        const roiMonths = Math.ceil(estimatedCost / monthlySavings);
        
        document.getElementById('monthly-savings').textContent = '$' + monthlySavings.toFixed(0);
        document.getElementById('annual-savings').textContent = '$' + annualSavings.toFixed(0);
        document.getElementById('five-year-savings').textContent = '$' + fiveYearSavings.toFixed(0);
        document.getElementById('roi-timeline').textContent = roiMonths + '-' + (roiMonths + 6) + ' months';
    }
    
    sqftInput.addEventListener('input', calculateROI);
    billInput.addEventListener('input', calculateROI);
    typeSelect.addEventListener('change', calculateROI);
    
    // Initial calculation
    calculateROI();
    
    // Before/After Slider functionality
    const sliders = document.querySelectorAll('[data-comparison]');
    sliders.forEach(slider => {
        let isDown = false;
        const handle = slider.querySelector('.slider-handle');
        
        slider.addEventListener('mousedown', () => isDown = true);
        slider.addEventListener('mouseup', () => isDown = false);
        slider.addEventListener('mouseleave', () => isDown = false);
        
        slider.addEventListener('mousemove', (e) => {
            if (!isDown) return;
            const x = e.offsetX;
            const width = slider.offsetWidth;
            const percent = (x / width) * 100;
            handle.style.left = percent + '%';
        });
    });
});
</script>